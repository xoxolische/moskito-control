{
  "mailNotificationEnabled":false,
  "notificationsMutingTime":60,
  "defaultApplication":"Burgershop",
  "@applications":[
    {
      "name":"Burgershop",
      "@components":[
        {
          "name":"test",
          "category":"shop",
          "connectorType":"HTTP",
          "location":"localhost:8080/burgershop/"
        }
      ],
      "@charts":[
        {
          "name":"Sessions",
          "limit":100,
          "@lines":[
            {
              "component":"test",
              "accumulator":"SessionCount Cur Absolute"
            }
          ]
        },
        {
          "name":"CPU",
          "limit":100,
          "@lines":[
            {
              "component":"Test",
              "accumulator":"CPU Time 1m"
            }
          ]
        },
        {
          "name":"URL AVG",
          "limit":100,
          "@lines":[
            {
              "component":"Test",
              "accumulator":"URL AVG 1m"
            }
          ]
        }
      ]
    }
  ],
  "@connectors":[
    {
      "type":"HTTP",
      "className":"org.moskito.control.connectors.HttpConnector"
    },
    {
      "type":"RMI",
      "className":"org.moskito.control.connectors.RMIConnector"
    }
  ],
  "@statusUpdater":{
    "checkPeriodInSeconds":10,
    "threadPoolSize":10,
    "timeoutInSeconds":60,
    "enabled":true
  },
  "@chartsUpdater":{
    "checkPeriodInSeconds":40,
    "threadPoolSize":5,
    "timeoutInSeconds":60,
    "enabled":true
  }
}